{% extends 'base.html.twig' %}

{% block title %}Env{% endblock %}

{% block body %}
    <h1>Env</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ env.id }}</td>
            </tr>
            <tr>
                <th>Uuid</th>
                <td>{{ env.uuid }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ env.name }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_env_index') }}" class="btn btn-secondary">Back to list</a>
    <br>
    <a href="{{ path('app_env_edit', {'id': env.id}) }}" class="btn btn-primary">Edit</a>

    {{ include('env/_delete_form.html.twig') }}
{% endblock %}
